{
  "name": "react-network-diagrams",
  "version": "1.0.1",
  "keywords": [
    "network",
    "circuits",
    "maps",
    "concatenated",
    "parallel",
    "editor",
    "react"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "source": "src/index.js",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "homepage": "http://software.es.net/react-network-diagrams",
  "author": "ESnet Tools Team <tools@es.net>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esnet/react-network-diagrams.git"
  },
  "bugs": {
    "url": "https://github.com/esnet/react-network-diagrams/issues"
  },
  "scripts": {
    "docs": "echo \"*** Building API docs\n\" && react-docgen src/components -x js -o ../website/src/api/docs.json --pretty",
    "lint": "eslint src/components/*.js",
    "test": "npm run lint",
    "watch": "parcel watch",
    "build": "echo \"*** Building dist\n\" && rm -rf dist/* && parcel build",
    "prepublishOnly": "rm -rf ./dist && npm run build",
    "start-website": "react-scripts start",
    "build-website": "echo \"*** Building website\n\" && rm -rf docs && react-scripts build && mv build docs",
    "test-build": "react-scripts build",
    "precommit": "npm run lint && npm run prettier",
    "prettier": "npx prettier . --write"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "license": "BSD-3-Clause-LBNL",
  "dependencies": {
    "d3-scale": "^4.0.2",
    "keymirror": "^0.1.1",
    "prop-types": "^15.8.1",
    "react-select": "^5.8.0",
    "underscore": "^1.13.6",
    "victor": "^1.1.0"
  },
  "devDependencies": {
    "parcel": "^2.10.3",
    "prettier": "3.1.0",
    "react": "^18.2.0",
    "react-docgen": "^7.0.1",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "peerDependencies": {
    "react": "^16.0 || ^17.0 || ^18.0",
    "react-dom": "^16.0 || ^17.0 || ^18.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "description": "This repository contains an initial set of React circuit drawing and network mapping components which are used within the ESnet Portal, but are not tied to ESnet, or even to network visualization."
}
